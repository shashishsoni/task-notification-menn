{
  "version": 2,
  "builds": [
    {
      "src": "server/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server/index.js"
    },
    {
      "src": "/components/(.*)",
      "dest": "build/components/$1"
    },
    {
      "src": "/(.*)",
      "dest": "build/$1"
    },
    {
      "src": "/",
      "dest": "build/index.html"
    },
    {
      "src": "/name/createuserpage",
      "dest": "build/createuserpage.html"
    }
  ]
}
